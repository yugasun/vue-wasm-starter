{
  "name": "vue-ts-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "crate:build": "cd crates && wasm-pack build --target web --release",
    "crate:dev": "cd crates && cargo watch -s 'wasm-pack build --target web --dev' -C",
    "crate": "pnpm run crate:${MODE:-build}",
    "crates:greet": "pnpm run crate greet",
    "dev:crates": "MODE=dev run-p crates:*",
    "dev:vite": "vite --host",
    "dev": "run-p dev:*",
    "build:crates": "run-s crates:*",
    "build:vite": "vite build",
    "build": "run-s build:crates build:vite",
    "build:analyze": "cross-env vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx msw init public --save",
    "prepare": "husky install",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run",
    "test:unit:update": "vitest run --update",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@heroicons/vue": "^2.1.5",
    "@unocss/reset": "^0.53.6",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^10.11.1",
    "axios": "^1.7.7",
    "element-plus": "^2.8.1",
    "pinia": "^2.2.2",
    "universal-cookie": "^4.0.4",
    "vue": "^3.5.1",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.2.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@pinia/testing": "^0.1.5",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.5",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.10",
    "msw": "^1.3.3",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.45",
    "postcss-import": "^16.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rollup": "^4.21.2",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.11.6",
    "vite": "^5.4.3",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
